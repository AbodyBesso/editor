{
	"$schema": "http://json-schema.org/draft-07/schema",
	"properties": {
		"minecraft:biome": {
			"required": ["description"],
			"additionalProperties": false,
			"properties": {
				"description": {
					"required": ["identifier"],
					"additionalProperties": false,
					"properties": {
						"identifier": {
							"anyOf": [
								{
									"$ref": "../project/fileIdentifierEnum.json"
								},
								{
									"type": "string"
								}
							]
						}
					}
				},
				"components": {
					"$ref": "./components.json"
				}
			}
		}
	}
}
