{
	"$schema": "http://json-schema.org/draft-07/schema",
	"properties": {
		"minecraft:combat_regeneration": {
			"description": "Gives Regeneration I and removes Mining Fatigue from the mob that kills the Actor's attack target.",
			"properties": {
				"regeneration_duration": {
					"description": "The duration in seconds of Regeneration I added to the mob.",
					"type": "integer"
				}
			}
		},
		"minecraft:behvaior.play_dead": {
			"properties": {
				"priority": {
					"type": "integer"
				},
				"duration": {
					"type": "integer"
				},
				"force_below_health": {
					"type": "integer"
				},
				"random_start_chance": {
					"type": "number"
				},
				"random_damage_range": {
					"type": "array",
					"minItems": 2,
					"maxItems": 2,
					"items": {
						"type": "integer"
					}
				},
				"damage_sources": {
					"type": "array",
					"items": {
						"$ref": "../../general/vanilla/damageType.json"
					}
				},
				"apply_regeneration": {
					"type": "boolean"
				}
			}
		},
		"minecraft:breedable": {
			"properties": {
				"transform_to_item": {
					"description": "The breed item used will transform to this item upon successful interaction.",
					"$ref": "../../general/reference/identifiers.json#/definitions/item_identifiers"
				}
			}
		}
	}
}
